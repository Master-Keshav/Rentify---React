{
  "version": 2,
  "builds": [
    {
      "src": "rentify/client/vite.config.ts",
      "use": "@vercel/node"
    },
    {
      "src": "rentify/client/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*).(json|ico)",
      "dest": "/rentify/client/$1.$2",
      "headers": {
        "Cache-Control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/rentify/client/index.html"
    }
  ]
}
